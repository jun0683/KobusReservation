function fc_ChangeDate(obj) {

	var year, month, day, date;
	year  = obj.Tim_date_Year.options[obj.Tim_date_Year.selectedIndex].value;
	month = obj.Tim_date_Month.options[obj.Tim_date_Month.selectedIndex].value;
	day   = obj.Tim_date_Day.options[obj.Tim_date_Day.selectedIndex].value;
	date  = obj.Tim_date_Day;

	date.length = checkMonth(year, month);
	for(i=0; i<date.length; i++) {
		date.options[i].value = addZero(i + 1);
		date.options[i].text  = i + 1;
	}
	if(parseInt(day) > date.length) {
		date.selectedIndex = 0;
	}

}

