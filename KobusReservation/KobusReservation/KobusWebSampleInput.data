function  fc_Select(flag){

	var d	= document.InputForm;

	
	if(flag == "2"){

		Child = d.pCnt_050.selectedIndex;
		Man   = d.pCnt_100.selectedIndex;
		Bus_cod = d.BUS_GRA_I.selectedIndex ;
		Total = Child + Man;
	
		if(d.TER_FR.selectedIndex == 0) {
			alert("출발지를 선택하세요");
			d.TER_FR.focus();
			return false;
		}
	
		if(d.TER_TO.selectedIndex == -1 || (d.TER_TO.selectedIndex == 0 && d.TER_TO.options[d.TER_TO.selectedIndex].value == "")) {
			alert("도착지를 선택하세요");
			d.TER_TO.focus();
			return false;
		}
	
		if(d.TIM_TIM_I.selectedIndex == 0) {
			alert("출발시간을 선택하세요");
			d.TIM_TIM_I.focus();
			return false;
		}
	
		if(Bus_cod == 0 || Bus_cod == 1 || Bus_cod == 4){
	        	ticket_num = 45;
		}else {
			ticket_num = 27;
		}
	
		if(Total > ticket_num){
			alert(" 해당 버스등급에 좌석수 (" + ticket_num + ") 을 초과하였습니다.");
			d.pCnt_100.selectedIndex =0;
			d.pCnt_100.focus();
			return false;
		}
	
		fc_CadRes();
	}else if(flag == "1"){
		d.method = "post";
        d.action = "./01_info/info16.jsp";
        d.submit();
	}else{
		alert("고속버스 홈티켓은 전산 사정으로 인해 잠시 홈티켓 작업을 중단합니다. 조회/발행/재발행/취소는 가능합니다.");
	}

}

////이지인터넷연계추가소스
function fc_CadRes(){

	var frm = document.InputForm;
	var d1 = document.easy;

	var sTim_day =  frm.Tim_date_Year.options[frm.Tim_date_Year.selectedIndex].value +
				frm.Tim_date_Month.options[frm.Tim_date_Month.selectedIndex].value +
				frm.Tim_date_Day.options[frm.Tim_date_Day.selectedIndex].value +
				frm.TIM_TIM_I.options[frm.TIM_TIM_I.selectedIndex].value;

	var sToday = frm.Tim_date_Year.options[frm.Tim_date_Year.selectedIndex].value +
				frm.Tim_date_Month.options[frm.Tim_date_Month.selectedIndex].value +
				frm.Tim_date_Day.options[frm.Tim_date_Day.selectedIndex].value ;

	if(frm.pCnt_100.selectedIndex == 0 && frm.pCnt_050.selectedIndex == 0){
		alert("예약 매수를 선택하세요");
		frm.pCnt_100.focus();
		return false;
	}

	if(sTim_day <= 201110042228){
		alert("출발 한시간 이전에만 예약 하실수 있습니다.");
		frm.Tim_date_Year.focus();
		return false;
	}
	
	//이지인터넷연계추가소스
	if(frm.TER_FR.value == "310" || frm.TER_FR.value == "020" || frm.TER_FR.value == "032" || frm.TER_FR.value == "190" || frm.TER_FR.value == "456") {
		if (confirm("               출발지 중 센트럴*ⓔ[020], 천안*ⓔ[310], 동서울*ⓔ[032], 안산*ⓔ[190],                 \n\n               정안(휴)*ⓔ[456] 출발노선은 저희 코버스 사이트에서 예약을 하실 수가 없습니다.                 \n\n               위의 출발지에서 예약을 하기 원하 시면 아래의 <확인> 버튼을 클릭하십시오.                 \n\n               <확인> 버튼을 클릭하시면  www.hticket.co.kr [이지티켓] 사이트로 넘어가셔서                 \n\n               예약을 진행 하시게 됩니다.(만일, <확인>버튼을 누르셨을때 사이트가 열리지 않는다면                 \n\n               ☎(02)2088-2635 번 으로 문의하시기 바랍니다.)                 \n\n               예약을 원하시지 않는다면 <아니오> 버튼을 클릭 하시고 다른 노선을 조회 해 주십시요.") == true)
		{
			var vsTer_fr = '0' + frm.TER_FR.value;
			var vsTer_to = '0' + frm.TER_TO.value;
			var vsTim_date = sToday;
			var vsTim_tim = frm.TIM_TIM_I.value;
			
			
			d1.searchStTermNbr.value = vsTer_fr;
			d1.searchArTermNbr.value = vsTer_to;
			d1.searchDate.value = vsTim_date;
